    name: "libstagefright_soft_c2aacenc",
    vendor_available: true,

    srcs: ["C2SoftAacEnc.cpp"],

    cflags: ["-Werror"],

    sanitize: {
        misc_undefined: [
            "signed-integer-overflow",
            "unsigned-integer-overflow",
